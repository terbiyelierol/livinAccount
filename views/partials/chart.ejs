<div class="row">
  <div class="col s12 l4 m7 push-l4">
    <div class="card">
      <div class="card-image">
        <img class="card_image"src="../images/undraw_source_code_re_wd9m.svg">
      </div>
      <div class="card-action center">
        <a class="waves-effect light-blue darken-4 btn modal-trigger" href="#modal1"><i class="fa fa-fast-forward" aria-hidden="true"></i></a>
        <div id="modal1" class="modal bottom-sheet">
          <div class="modal-content">
          <article>
            <h4>Mission completed</h4>
            <p><span style="font-weight: bold;">The Quote:</span> <%=content.content%></p>
            <p><i class=" light-blue-text darken-4 fa fa-user" aria-hidden="true"></i> <%=name%></p>
          </article>
          </div>
          <div class="modal-footer">
            <a href="/main" class="modal-close light-blue darken-4 btn-flat">Close</a>
          </div>
        </div>      
      </div>
    </div>
  </div>
</div>

  <section class="container section hide" id="section">
    <div class="row">
      <div class="col s12 l6 m7 push-l3 z-depth-4 " >
        <h5>Expense Type Chart</h5>
        <canvas id="myChart" width="600" height="300"></canvas>
      </div>
    </div>
  </section>




